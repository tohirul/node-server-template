    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.14",
    "@types/winston": "^2.4.4",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "module-alias": "^2.2.3",
