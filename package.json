{
  "name": "project_management_server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "https://github.com/tohirul/project-management-server",
  "author": "tohriul",
  "license": "MIT",
  "_moduleAliases": {
    "@": "dist",
    "@/app": "dist/app",
    "@/config": "dist/config",
    "@/core": "dist/core",
    "@/database": "dist/database",
    "@/routes": "dist/routes"
  },
  "scripts": {
    "seed": "ts-node prisma/seed.ts",
    "build": "rimraf dist && npx tsc",
    "start": "yarn build && node dist/server.js",
    "server": "yarn build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/server.ts\""
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
